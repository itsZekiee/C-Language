#include <stdio.h>
int main() {
    int num, i;
    char option;
    do
    {
        printf("Enter a positive integer: ");
        scanf("%d", &num);
        printf("Factors of %d are: \n", num);
        for (i = 1; i <= num; ++i) {
            if (num % i == 0) {
                printf("%d ", i);
            }
        }
        printf("do you like to try again? (Y or N)");
        scanf("%s", option);
    } while (option == "Y");
    
    return 0;
}

